    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.2.3",
    "@types/node-cache": "^4.2.5",
    "@types/puppeteer": "^7.0.4",
    "@types/user-agents": "^1.0.2",
    "chokidar": "^3.5.3",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
