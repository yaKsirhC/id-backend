  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npx concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "npx swc src -w --out-dir dist",
    "watch-dev": "npx nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "npx swc src --copy-files -d dist",
    "start": "npx swc src --copy-files -d dist && node dist/index.js"
  },
